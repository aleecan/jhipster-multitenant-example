<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity MonthlyLine.
    -->
    <changeSet id="20201130011636-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="complete_line_no_id"
                                 baseTableName="monthly_line"
                                 constraintName="fk_monthly_line_complete_line_no_id"
                                 referencedColumnNames="id"
                                 referencedTableName="monthly_line"/>

        <addForeignKeyConstraint baseColumnNames="difference_line_no_id"
                                 baseTableName="monthly_line"
                                 constraintName="fk_monthly_line_difference_line_no_id"
                                 referencedColumnNames="id"
                                 referencedTableName="monthly_line"/>

        <addForeignKeyConstraint baseColumnNames="positive_value_line_no_id"
                                 baseTableName="monthly_line"
                                 constraintName="fk_monthly_line_positive_value_line_no_id"
                                 referencedColumnNames="id"
                                 referencedTableName="monthly_line"/>

        <addForeignKeyConstraint baseColumnNames="negatif_value_line_no_id"
                                 baseTableName="monthly_line"
                                 constraintName="fk_monthly_line_negatif_value_line_no_id"
                                 referencedColumnNames="id"
                                 referencedTableName="monthly_line"/>

    </changeSet>
</databaseChangeLog>
